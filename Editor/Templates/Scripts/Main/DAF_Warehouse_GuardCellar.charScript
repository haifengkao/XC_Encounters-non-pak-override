INITCHARACTER:__MeTRIGGER:%Point1 = DAF_Warehouse_StandOnLid_ee99946f-dc8c-4191-9fb5-672c94c2cdedTRIGGER:%Point2 = DAF_Warehouse_TalkToLeonard_2912140f-d9ce-4759-b312-fc4429e0e998TRIGGER:%Point3 = DAF_PointVillageWarehouseOut_3cfc518b-d0ad-4964-9041-28897119e651CHARACTER:%Leonard = DAF_Leonard_3e60eaa9-a33f-4b53-b8f7-81bc93672dd9BEHAVIOURREACTION Main, 500USAGE PEACEVARS	FLOAT:_distACTIONS	CharacterMoveTo(%Point2)	CharacterLookFrom(%Point2)	IF "!c1&c2&c3&!c4"		CharacterIsDead(%Leonard)		GetDistance(_dist,__Me,%Leonard)		IsLessThen(_dist,6.0)		IsInDialog(%Leonard)
	THEN		DialogStart("DAF_AD_Warehouse_GuardWhine",__Me,%Leonard)		CharacterPlayAnimation("Sigh_01")	ENDIF	Sleep(2)	CharacterMoveTo(%Point3)	CharacterLookFrom(%Point3)	CharacterPlayAnimation("Annoyed_01")	Sleep(5)	CharacterPlayAnimation("Bored_01")	Sleep(5)	CharacterPlayAnimation("Annoyed_01")	DialogStart("DAF_AD_Warehouse_GuardWhine2",__Me)	CharacterPlayAnimation("Annoyed_01")	Sleep(10)	CharacterMoveTo(%Point1)	CharacterLookFrom(%Point1)	Sleep(2)	CharacterPlayAnimation("Attention_01")	DialogStart("DAF_AD_Warehouse_GuardWhine3",__Me)	CharacterPlayAnimation("Sigh_01")	Sleep(2)	CharacterMoveTo(%Point3)	CharacterLookFrom(%Point3)	CharacterPlayAnimation("Bored_02")	Sleep(5)	CharacterPlayAnimation("Dust_Off_01")	Sleep(5)	CharacterPlayAnimation("Bored_01")	Sleep(5)	CharacterPlayAnimation("Annoyed_01")	Sleep(5)	CharacterPlayAnimation("Yawn_01")	Sleep(5)	CharacterPlayAnimation("Bored_03")	Sleep(5)EVENTSEVENT CombatStopsDialogON	OnCombatStarted()ACTIONS	DialogRequestStop("DAF_AD_Warehouse_GuardWhine")	DialogRequestStop("DAF_AD_Warehouse_GuardWhine2")	DialogRequestStop("DAF_AD_Warehouse_GuardWhine3")